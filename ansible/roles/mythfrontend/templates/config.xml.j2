<Configuration>
  <UPnP>
    <UDN>
      <MediaRenderer>aebc662d-8a7e-4ffc-b1cd-b52d93cd8f16</MediaRenderer>
    </UDN>
  </UPnP>
  <LocalHostName>my-unique-identifier-goes-here</LocalHostName>
  <Database>
    <PingHost>1</PingHost>
    <Host>{{ db.host }}</Host>
    <UserName>{{ db.user }}</UserName>
    <Password>{{ vault_mythdb_password }}</Password>
    <DatabaseName>{{ db.name }}</DatabaseName>
    <Port>0</Port>
  </Database>
  <WakeOnLAN>
    <Enabled>0</Enabled>
    <SQLReconnectWaitTime>0</SQLReconnectWaitTime>
    <SQLConnectRetry>5</SQLConnectRetry>
    <Command>echo 'WOLsqlServerCommand not set'</Command>
  </WakeOnLAN>
</Configuration>
